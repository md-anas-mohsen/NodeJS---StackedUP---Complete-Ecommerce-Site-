    "bcrypt": "5.0.1",
    "cloudinary": "^1.26.2",
    "cookie-parser": "1.4.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-fileupload": "^1.2.1",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.13.2",
    "nodemailer": "6.6.2",
    "stripe": "^8.167.0",
    "validator": "13.6.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.9"
  }
}
